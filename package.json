{
  "name": "lixa-gallery",
  "private": true,
  "version": "0.5.0",
  "type": "module",
  "scripts": {
    "postinstall": "svelte-kit sync",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
